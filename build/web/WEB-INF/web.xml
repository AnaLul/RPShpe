<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CargarArchivo</servlet-name>
        <servlet-class>Control.CargarArchivo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Carga</servlet-name>
        <servlet-class>Control.Carga</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>Control.Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResetDB</servlet-name>
        <servlet-class>Control.ResetDB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CargarArchivo</servlet-name>
        <url-pattern>/CargarArchivo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Carga</servlet-name>
        <url-pattern>/Carga</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/Upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResetDB</servlet-name>
        <url-pattern>/ResetDB</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
